--functions
function allCodes()

local args = {
    [1] = "15KLIKESTHANKYOU"
}

game:GetService("ReplicatedStorage").Events.ClaimCode:FireServer(unpack(args))

local args = {
    [1] = "update2"
}

game:GetService("ReplicatedStorage").Events.ClaimCode:FireServer(unpack(args))

local args = {
    [1] = "update1"
}

game:GetService("ReplicatedStorage").Events.ClaimCode:FireServer(unpack(args))

local args = {
    [1] = "release"
}

game:GetService("ReplicatedStorage").Events.ClaimCode:FireServer(unpack(args))

local args = {
    [1] = "testing"
}

game:GetService("ReplicatedStorage").Events.ClaimCode:FireServer(unpack(args))

end
function WinterAutoClick()
	while getgenv().WAC == true do
		local args = {
			[1] = "Events"
		}

game:GetService("ReplicatedStorage").Events.Tap:FireServer(unpack(args))
wait(0.000000000000000000000000000000000000000001)
end
end
function AutoClick()
    while getgenv().AC == true do
        local args = {
        [1] = "Main"
    }

game:GetService("ReplicatedStorage").Events.Tap:FireServer(unpack(args))
wait(0.000000000000000000000000000000000000000001)

end
end
function AutoHatch()
	while getgenv().AH == true do
		local args = {
			[1] = {},
			[2] = getgenv().petType,
			[3] = 1
		}

		game:GetService("ReplicatedStorage").Events.HatchEgg:InvokeServer(unpack(args))

	end		
end
function AutoRebirth()
	while getgenv().AR == true do
	local args = {
    	[1] = getgenv().rebirthAmount
	}

		game:GetService("ReplicatedStorage").Events.Rebirth:FireServer(unpack(args))
		wait(000000000000000000000000000000000000000001)
	end
end
